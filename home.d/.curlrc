# ~/.curlrc -- Default config file for `curl'

# Disguise as IE 9 on Windows 7.
user-agent = "Mozilla/5.0 (compatible; MSIE 9.0; Windows NT 6.1; Trident/5.0)"
#user-agent="Googlebot/2.1 (+http://www.google.com/bot.html)"

# automatically set the previous URL as referer
referer=";auto"

# Maximum connection time to take
connect-timeout=10

# Maximum whole time (seconds) to take
#max-time=90

# use with `silent` showing an error message if it fails
#show-error

# moved to a different location
#location

#verbose
